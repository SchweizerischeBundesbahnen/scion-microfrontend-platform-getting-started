    "prebuild": "tsc --noEmit"
  },
  "dependencies": {
    "@scion/microfrontend-platform": "^1.0.0 || ^1.0.0-rc.0 || ^1.0.0-beta.0",
    "@scion/toolkit": "^10.0.0 || ^10.0.0-rc.0 || ^10.0.0-beta.0",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@types/node": "^12.12.50",
    "cpy-cli": "^3.1.1",
    "parcel": "^1.12.4",
    "sass": "^1.26.8",
    "typescript": "^3.9.5"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
